./usr/lib/jvm/java-8-oracle/db/lib/derbytools.jar
./usr/lib/jvm/java-8-oracle/db/lib/derbyoptionaltools.jar
./usr/lib/jvm/java-7-openjdk-amd64/lib/tools.jar
./usr/lib/astah_professional/lib/tools.jar


